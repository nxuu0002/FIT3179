{
 "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
 "title": "COVID-19 Cumulative Case Count by State",
 "width": 800,
 "height": 600,
 "projection": {"type": "equirectangular"},
 "data": {
 "url": "https://github.com/nxuu0002/FIT3179/blob/main/Australia_state.json",
 "format": {"type": "topojson", "feature": "Australia_state"}
 },
 "transform": [
 {
 "lookup": "properties.NAME",
 "from": {
 "data": {
 "url": "https://github.com/nxuu0002/FIT3179/blob/main/data/case_count_by_state.csv"
 },
 "key": "State",
 "fields": ["Cumulative case count"]
 }
 }
 ],
 "mark": {"type": "geoshape"},
 "encoding": {
 "color": {
 "field": "Cumulative case count",
 "type": "quantitative",
 "scale": {
 "type": "threshold",
 "domain": [10000, 100000, 500000],
 "range": ["#fdbe85", "#fd8d3c", "#e6550d", "#a63603"]
 }
 },
 "tooltip": [
 {"field": "properties.NAME", "type": "nominal", "title": "State"},
 {"field": "Active", "type": "quantitative"}
 ]
 }
}
